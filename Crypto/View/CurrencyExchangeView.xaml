<UserControl x:Class="Crypto.View.CurrencyExchangeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Crypto.View"
             mc:Ignorable="d" 
             d:DesignHeight="635" d:DesignWidth="780">
    <Grid>
        <Border Style="{DynamicResource BorderTheme}" CornerRadius="0, 0, 20, 0" />
        <Grid>
            <TextBlock Text="Exchange" Style="{DynamicResource Text}" />
        </Grid>
    </Grid>
</UserControl>
